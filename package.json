{
    "name": "kinkoi-project",
    "version": "0.0.0",
    "license": "MIT",
    "engines": {
        "node": "20.10.0",
        "pnpm": "8.15.5",
        "npm": "please-use-pnpm",
        "yarn": "please-use-pnpm"
    },
    "packageManager": "pnpm@8.15.5",
    "scripts": {
        "clean": "rimraf \"**/node_modules\" && pnpm -r clean",
