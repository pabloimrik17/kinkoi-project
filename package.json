        "prepare": "husky install"
    },
    "private": true,
    "devDependencies": {
        "@commitlint/cli": "^19.2.2",
        "@commitlint/config-conventional": "^19.2.2",
        "@commitlint/types": "^19.0.3",
        "@nx/js": "18.2.4",
        "husky": "^8.0.3",
        "lint-staged": "^15.2.2",
        "nx": "18.2.4",
        "nx-cloud": "^18.0.0",
        "prettier": "^3.2.5",
        "prettier-plugin-organize-imports": "^3.2.4"
    }
}
